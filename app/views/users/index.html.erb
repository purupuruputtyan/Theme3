<%= render 'books/list', books: @books %>
<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
   <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
      <tr>
        <th></th>
        <th><%= user.name %></th>
        <th><%= link_to 'Show', user_path(user.id) %></th>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>